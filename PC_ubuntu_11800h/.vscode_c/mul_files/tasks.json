{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C: gcc-10 生成活动文件",
            "command": "/usr/bin/gcc-10",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${workspaceFolder}/EXECUTE/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        },
        {
            "type": "cppbuild",
            "label": "C_SIMD: gcc-10 生成活动文件",
            "command": "/usr/bin/gcc-10",
            "args": [
                "-fdiagnostics-color=always",
                // "-O1",
                // "-O2",
                "-O3",
                // "-Og",
                "-mavx",
                "-mavx2",
                "-I",
                "${fileDirname}/",
                "-g",
                "${file}",
                "${fileDirname}/encode.c",
                "-o",
                "${workspaceFolder}/EXECUTE/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}